
public class WhileToFor {
	
	public static void main(String[] args) {
		// c√≥digo while
		int contador = 0;
		while (contador <= 10) {
			System.out.println(contador);
			contador++;
		}
		
		//codigo for
		
		for(int contador = 0; contador <= 10; contador++){
			System.out.println(contador);
		}
	}

}
