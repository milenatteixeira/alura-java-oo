
public class WhileToFor {
	
	public static void main(String[] args) {
		// c√≥digo while
		int contador = 0;
		while (contador <= 10) {
			System.out.println(contador);
			contador++;
		}
		
		//codigo for		
		for(int contadorFor = 0; contadorFor <= 10; contadorFor++){
			System.out.println(contadorFor);
		}
	}

}
